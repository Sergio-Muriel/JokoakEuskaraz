    <Entry Name="prisoner_barks_quiet_001_karlspringer_001">... Ein Teufel... Ein Teufel für die französischen Teufel...</Entry>
    <Entry Name="prisoner_barks_quiet_002_karlspringer_001">In der Hölle... Der Hölle...</Entry>
    <Entry Name="prisoner_barks_quiet_003_karlspringer_001">.. Ich-wey-Nichts... Nichts...</Entry>
    <Entry Name="prisoner_barks_quiet_004_karlspringer_001">... eines Tages... Heimat... Frühling...</Entry>
    <Entry Name="prisoner_barks_quiet_005_karlspringer_001">Ich bin nur ein Soldat... das ist alles...</Entry>
    <Entry Name="prisoner_barks_moans_001_karlspringer_001">Hallo? Ist jemand da?</Entry>
    <Entry Name="prisoner_barks_moans_002_karlspringer_001">Sicher hier... in meinem Käfig...</Entry>
    <Entry Name="prisoner_barks_moans_003_karlspringer_001">Lass mich in Ruhe...</Entry>
    <Entry Name="prisoner_barks_scared_001_karlspringer_001">Nein! Nein! Lass zu da!</Entry>
    <Entry Name="prisoner_barks_scared_002_karlspringer_001">Schlieque es wieder!</Entry>
    <Entry Name="prisoner_barks_scared_003_karlspringer_001">Ich kann es hören!</Entry>
    <Entry Name="prisoner_barks_scared_004_karlspringer_001">Bitte! Bitte!</Entry>
    <Entry Name="prisoner_barks_scared_005_karlspringer_001">Ich will nicht so sterben!</Entry>
    <Entry Name="prisoner_barks_scared_006_karlspringer_001">Schlieque es wieder!</Entry>
    <Entry Name="prisoner_barks_scared_007_karlspringer_001">Ich kann es hören!</Entry>
    <Entry Name="prisoner_barks_gas_001_karlspringer_001">[Eztulka]</Entry>
    <Entry Name="prisoner_barks_gas_001_karlspringer_002">[Eztulka]</Entry>
    <Entry Name="prisoner_barks_gas_001_karlspringer_003">Ich kriege keine Luft!</Entry>
    <Entry Name="prisoner_barks_gas_001_karlspringer_004">[Eztula, itolarria]</Entry>
    <Entry Name="prisoner_barks_gas_001_karlspringer_005">Helfen Sie mir! Bitte! So will ich nicht sterben!</Entry>
    <Entry Name="prisoner_barks_gas_001_karlspringer_006">[Eztulka]</Entry>
    <Entry Name="prisoner_barks_gas_001_karlspringer_007">Mein Gott... Bitte...</Entry>
    <Entry Name="prisoner_barks_gas_001_karlspringer_008">[Eztulka... hil da]</Entry>
    <Entry Name="prisoner_barks_monsterattack_001_karlspringer_001">AHHHH! Du Teufel! Du Unhold! NEIN - BITTE - NEIN!</Entry>
    <Entry Name="prisoner_barks_quiet_006_karlspringer_001">... Ein Teufel... Ein Teufel für die französischen Teufel...</Entry>
    <Entry Name="prisoner_barks_quiet_007_karlspringer_001">... in der Hölle... der Hölle...</Entry>
    <Entry Name="prisoner_barks_quiet_008_karlspringer_001">.. Ich-wey-Nichts... Nichts...</Entry>
    <Entry Name="prisoner_barks_quiet_009_karlspringer_001">... eines Tages... Heimat... Frühling...</Entry>
    <Entry Name="prisoner_barks_quiet_010_karlspringer_001">Ich bin nur ein Soldat... das ist alles...</Entry>
    <Entry Name="prisoner_barks_moans_004_karlspringer_001">Hallo?</Entry>
    <Entry Name="prisoner_barks_moans_005_karlspringer_001">Sicher hier... in meinem Käfig...</Entry>
    <Entry Name="prisoner_barks_moans_006_karlspringer_001">Lass mich in Ruhe...</Entry>
    <Entry Name="prisoner_barks_scared_008_karlspringer_001">Kommt da!</Entry>
    <Entry Name="prisoner_barks_panicked_001_karlspringer_001">NEIN! NEIN! GOTT IM HIMMEL!</Entry>
    <Entry Name="prisoner_barks_panicked_002_karlspringer_001">BITTE! BITTE! HILF MIR!</Entry>
    <Entry Name="prisoner_barks_panicked_003_karlspringer_001">NEIN! NEIN!</Entry>
    <Entry Name="prisoner_barks_panicked_004_karlspringer_001">Kommt da!</Entry>
    <Entry Name="prisoner_barks_panicked_005_karlspringer_001">Kommt da!</Entry>
    <Entry Name="prisoner_barks_panicked_006_karlspringer_001">ICH WILL NICHT SO STERBEN! BITTE! BITTE!</Entry>
    <Entry Name="prisoner_barks_panicked_007_karlspringer_001">NEIN! NEIN! LASS ESS ZU!</Entry>
    <Entry Name="prisoner_barks_gas_002_karlspringer_001">[Eztula, itolarria]</Entry>
    <Entry Name="prisoner_barks_gas_003_karlspringer_001">Ich kann nicht atmen!</Entry>
    <Entry Name="prisoner_barks_gas_004_karlspringer_001">[Eztulka]</Entry>
    <Entry Name="prisoner_barks_gas_005_karlspringer_001">Mein Gott... bitte...</Entry>
    <Entry Name="prisoner_barks_gas_006_karlspringer_001">[Eztulka... hil da]</Entry>
